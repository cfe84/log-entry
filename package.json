{
	"name": "log-entry",
	"displayName": "log-entry",
	"description": "Command to create a markdown log entry with a date format",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:le.createLogEntry"
	],
	"main": "./out/infrastructure/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "le.createLogEntry",
				"title": "Create log entry",
				"category": "Log Entry"
			}
		],
		"keybindings": []
	},
	"scripts": {
		"vscode:prepublish": "npm run build",
		"build": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"test": "mocha --require ts-node/register ./test/**/*.ts"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"typescript": "^3.8.3"
	},
	"publisher": "cfe",
	"repository": {
		"url": "https://github.com/cfe84/proletarian-wizard"
	},
	"dependencies": {
		"moment": "^2.27.0"
	}
}
